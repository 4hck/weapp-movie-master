<view class='container'>
  <view class='search-city'></view>
  <view class='citylist-content'>
    <view wx:for='{{citylist}}' wx:for-item='section' wx:key='{{section.title}}' data-index='{{section.title}}' class='section'>
      <view class='section-title' data-index='{{section.title}}'>{{section.title}}</view>
      <view class='{{section.style==="inline"?"section-body-inline":"section-body"}}'>
        <view wx:for='{{section.items}}'wx:for-item='city' wx:key='{{city.id}}' class='section-item' bindtap='selectCity' data-city='{{city}}'>{{city.city_name}}</view>
      </view>
    </view>
  </view>
  <view class='citylist-nav' id='test'>
    <view wx:for='{{citylist}}' wx:key='{{item.title}}' bindtap='navSelect' data-index='{{item.title}}'>{{item.style==='inline'?item.index:item.title}}</view>
  </view>
</view>